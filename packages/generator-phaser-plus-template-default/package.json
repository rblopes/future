{
  "name": "@rblopes/generator-phaser-plus-template-default",
  "description": "Create Phaser Web games with ease.",
  "version": "1.0.0-beta.6",
  "homepage": "https://github.com/rblopes/generator-phaser-plus/tree/master/packages/generator-phaser-plus-template-default#readme",
  "author": "Rafael Barbosa Lopes (https://github.com/rblopes)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/rblopes/generator-phaser-plus.git"
  },
  "bugs": "https://github.com/rblopes/generator-phaser-plus/issues",
  "keywords": [
    "yeoman-generator",
    "babel",
    "browsersync",
    "canvas",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "es6",
    "es7",
    "es8",
    "esnext",
    "game development",
    "gamedev",
    "games",
    "gulp",
    "html5",
    "phaser",
    "scaffold",
    "webgl",
    "webpack"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "files": [
    "templates/",
    "index.js",
    "test.js"
  ],
  "scripts": {
    "fix": "xo --fix",
    "test": "xo && cross-env NODE_ENV=test mocha --reporter spec"
  },
  "engines": {
    "node": ">= 8.7"
  },
  "dependencies": {
    "yeoman-generator": "^4.0.1"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "mocha": "^6.2.0",
    "xo": "^0.24.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "xo": {
    "envs": [
      "mocha",
      "node"
    ],
    "esnext": true,
    "ignores": [
      "templates/**"
    ],
    "overrides": [
      {
        "files": "test.js",
        "rules": {
          "promise/prefer-await-to-then": "off"
        }
      }
    ],
    "rules": {
      "capitalized-comments": "off",
      "quotes": [
        "error",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ]
    },
    "space": true
  },
  "license": "MIT"
}
